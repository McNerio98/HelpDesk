	<queryString>
		<![CDATA[select rq.anombre,to_char(rq.fecha,'dd-MM-yy HH:MI AM') fecha, e.nombre empresa, d.deptoname depto,
concat(u1.firstname,' ',u1.lastname) solicitante, concat(u2.firstname,' ',u2.lastname) autorizador,
to_char(rq.fechaaprovacion,'dd-MM-yy HH:MI AM') fechaAprov, 
concat(u3.firstname,' ',u3.lastname) contador
, concat('$ ',rq.total) total 
from requisicionespagos rq
inner join empresas e on e.idempresa = rq.idempresa
inner join departments d on d.iddepto = rq.iddepto
inner join users u1 on u1.iduser = rq.idcreador
inner join users u2 on u2.iduser = rq.idautorizador
inner join users u3 on u3.iduser = rq.idcontador 
where idrequisicion = $P{ID_Req}::Integer;]]>
	</queryString>
